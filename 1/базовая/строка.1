#!1
системная
используется
  с-префиксом rkt: racket/base
предоставлять строка строка? добавить-строки длина-строки подстрока прописные строчные \
  строки-равны? строки-возрастают? строки-не-убывают? строки-убывают? строки-не-возрастают? \
  строки-равны?/без-регистра строки-возрастают?/без-регистра строки-не-убывают?/без-регистра \
  строки-убывают?/без-регистра строки-не-возрастают?/без-регистра строки-равны?/местные \
  строки-возрастают?/местные строки-убывают?/местные строки-равны?/местные/без-регистра \
  строки-возрастают?/местные/без-регистра строки-убывают?/местные/без-регистра элемент-строки \
  установить-элемент-строки!

строка . литеры =
  rkt:apply rkt:string литеры

строка? объект =
  rkt:string? объект

элемент-строки строка позиция =
  rkt:string-ref строка позиция

установить-элемент-строки! строка позиция литера =
  rkt:string-set! строка позиция литера

добавить-строки . строки =
  rkt:apply rkt:string-append строки

подстрока строка начало (конец длина-строки(строка)) =
  rkt:substring строка начало конец

длина-строки строка =
  rkt:string-length строка

прописные строка =
  rkt:string-upcase строка

строчные строка =
  rkt:string-downcase строка

строки-равны? . строки =
  rkt:apply rkt:string=? строки

строки-возрастают? . строки =
  rkt:apply rkt:string<? строки

строки-не-убывают? . строки =
  rkt:apply rkt:string<=? строки

строки-убывают? . строки =
  rkt:apply rkt:string>? строки

строки-не-возрастают? . строки =
  rkt:apply rkt:string>=? строки

строки-равны?/без-регистра . строки =
  rkt:apply rkt:string-ci=? строки

строки-возрастают?/без-регистра . строки =
  rkt:apply rkt:string-ci<? строки

строки-не-убывают?/без-регистра . строки =
  rkt:apply rkt:string-ci<=? строки

строки-убывают?/без-регистра . строки =
  rkt:apply rkt:string-ci>? строки

строки-не-возрастают?/без-регистра . строки =
  rkt:apply rkt:string-ci>=? строки

строки-равны?/местные . строки =
  rkt:apply rkt:string-locale=? строки

строки-возрастают?/местные . строки =
  rkt:apply rkt:string-locale<? строки

строки-убывают?/местные . строки =
  rkt:apply rkt:string-locale>? строки

строки-равны?/местные/без-регистра . строки =
  rkt:apply rkt:string-locale-ci=? строки

строки-возрастают?/местные/без-регистра . строки =
  rkt:apply rkt:string-locale-ci<? строки

строки-убывают?/местные/без-регистра . строки =
  rkt:apply rkt:string-locale-ci>? строки
