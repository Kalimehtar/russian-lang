#!1
системная
используется
  с-префиксом rkt: racket/base
  базовая/управление
предоставлять читая-файл записывая-файл

читая-файл имя обработка
  #:режим режим 'двоичный
  =
  mode = выбор режим
    (двоичный) 'binary
    (текстовый) 'text
  rkt:call-with-input-file имя обработка #:mode mode

записывая-файл имя обработка
  #:режим режим 'двоичный
  #:если-существует существует 'ошибка
  #:права права #o666
  =
  mode = выбор режим
    (двоичный) 'binary
    (текстовый) 'text
  exists = выбор существует
    (ошибка) 'error
    (добавить) 'append
    (обновить) 'update
    (заменить) 'replace
    (обрезать) 'truncate
    (обрезать/заменить) 'truncate/replace
  rkt:call-with-output-file имя обработка \
    #:mode mode \
    #:exists exists \
    #:permissions права
